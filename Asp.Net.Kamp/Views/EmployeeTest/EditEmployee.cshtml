@using Asp.Net.Kamp.Controllers
@model Class1

@{
    ViewData["Title"] = "EditEmployee";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1> API ile Personel Güncelleme Sayfası</h1>
<form method="post" >

    <label>ID</label>
    @Html.TextBoxFor(x=>x.ID,new{@class="form-control"})
    <br />
    <label>Personel</label>
    @Html.TextBoxFor(x=>x.Name,new{@class="form-control"})
    <br />
    <button btn btn-outline-success >Güncelle </button>

</form>

